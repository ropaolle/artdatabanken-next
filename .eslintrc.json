{
  "extends": [
    "next/core-web-vitals",
    "plugin:storybook/recommended"
  ],
  "ignorePatterns": [
    "**/src/types/database.types.ts"
  ],
  "rules": {
    "no-console": [
      "warn",
      {
        "allow": [
          "info",
          "error"
        ]
      }
    ],
    "no-unused-vars": "off"
  }
}
